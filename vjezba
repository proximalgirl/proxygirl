4. 

def vertikalna_udaljenost(T_1, T_2):
    x1, y1 = T_1
    x2, y2 = T_2
    return abs(y1 - y2)

x1, y1 = map(float, input("Unesite koordinate prve točke (x1 y1): ").split())
x2, y2 = map(float, input("Unesite koordinate druge točke (x2 y2): ").split())

T_1 = (x1, y1)
T_2 = (x2, y2)

vertikalna_udaljenost = vertikalna_udaljenost(T_1, T_2)

print(f"Vertikalna udaljenost između točke T1 i točke T2 je {vertikalna_udaljenost}")



