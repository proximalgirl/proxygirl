def interpunkcija(s):
    brojac_tocka= s.count(".")
    brojac_zarez= s.count(",")
    brojac_dvotocka= s.count(":")
    brojac_tockazarez= s.count(";")
    return print("U zadanom stringu se nalazi ",brojac_tocka," točaka, ",brojac_zarez," zareza, ",brojac_dvotocka," dvotočaka, ",brojac_tockazarez," točkezareza")

s = input("Molimo unesite niz znakova:")

interpunkcija(s)



