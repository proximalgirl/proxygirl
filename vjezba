from math import sqrt

unos_1 = int(input("Molimo unesite kordinatu x:"))
unos_2 = int(input("Molimo unesite kordinatu y:"))

def udaljenost_od_ishodišta(x,y):
    udaljenost = int(sqrt(x**2 + y**2))
    return udaljenost

print(udaljenost_od_ishodišta(unos_1,unos_2))



