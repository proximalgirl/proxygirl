def projekcija_na_os_x(T):
    x, y = T
    Tx = (x, 0)
    return Tx

x = float(input("Unesite x koordinatu točke T: "))
y = float(input("Unesite y koordinatu točke T: "))
T = (x, y)
Tx = projekcija_na_os_x(T)
print("Projekcija točke T na x os je:", Tx)

