from random import *

unos = int(input("Molimo unesite prirodan broj:"))

lista_1 = []
lista_2 = []
lista_3 = []
lista_4 = []
lista_5 = []

if unos > 0:
    for i in range(unos):
        lista_1.append(randint(0,10))
        lista_2.append(randint(0,10))
        lista_3.append(randint(0,10))
        lista_4.append(randint(0,10))
        lista_5.append(randint(0,10))

    suma_1 = sum(lista_1)
    suma_2 = sum(lista_2)
    suma_3 = sum(lista_3)
    suma_4 = sum(lista_4)
    suma_5 = sum(lista_5)

    '''
    Sustav provjere
    print(lista_1)
    print(lista_2)
    print(lista_3)
    print(lista_4)
    print(lista_5)
    
    print(suma_1)
    print(suma_2)
    print(suma_3)
    print(suma_4)
    print(suma_5)
    '''

    najveca = max(suma_1,suma_2,suma_3,suma_4,suma_5)

    if najveca == suma_1:
        print(lista_1)
    if najveca == suma_2:
        print(lista_2)
    if najveca == suma_3:
        print(lista_3)
    if najveca == suma_4:
        print(lista_4)
    if najveca == suma_5:
        print(lista_5)



