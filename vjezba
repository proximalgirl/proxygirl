2. 

def upper_lower(s):
    lower_count = 0
    upper_count = 0
    other_count = 0
    for c in s:
        if c.isupper():
            upper_count += 1
        elif c.islower():
            lower_count += 1
        else:
            other_count += 1
    return lower_count, upper_count, other_count

s = input("Unesite niz znakova: ")
lower, upper, other = upper_lower(s)
print("Broj malih slova:", lower)
print(upper ,"Broj velikih slova:")
print(other ,"ostala znaka")



-------------------------------------------------------------------------

2.  

def upper_lower(s):
    lower_count = 0
    upper_count = 0
    other_count = 0
    for c in s:
        if c.isupper():
            upper_count += 1
        elif c.islower():
            lower_count += 1
        else:
            other_count += 1
    return lower_count, upper_count, other_count

s = input("Unesite niz znakova: ")
lower, upper, other = upper_lower(s)
print("Broj malih slova:", lower)
print("Broj velikih slova:", upper)
print("Broj ostalih znakova:", other)



