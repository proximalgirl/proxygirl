6. 


from random import *

def generiraj():
    broj = randint(1,10)
    if broj % 2 == 0:
        return True
    elif broj % 2 != 0:
        return False

# Testni dio - print(generiraj())

unos = int(input("Molimo unesite 0 ako mislite da je broj paran a 1 ako smatrate da nije:"))

if unos == 0 or 1:
    if unos == 0:
        unos = True
    elif unos == 1:
        unos = False
    # Testni dio - print(unos)
    if generiraj() == True and unos == True:
        print("Pogodak")
    elif generiraj() == True and unos == False:
        print("Promašaj")
    elif generiraj() == False and unos == True:
        print("Promašaj")
    elif generiraj() == False and unos == False:
        print("Pogodak")


else:
    print("Pogrešan unos")

