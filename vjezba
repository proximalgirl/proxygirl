3.

import os

# Funkcija za sortiranje prema prezimenu
def sort_prezime(item):
    return item[1]

# Inicijaliziramo listu za spremanje podataka
osobe = []

# Otvaramo ulaznu datoteku
with open("oib.txt", "r") as ulaz:
    # Prolazimo kroz svaku liniju u datoteci
    for linija in ulaz:
        # Razdvajamo OIB i prezime ime
        oib, prezime_ime = linija.strip().split(" ")
        # Dodajemo podatke u listu
        osobe.append((oib, prezime_ime))

# Sortiramo listu prema prezimenu
osobe.sort(key=sort_prezime)

# Otvaramo izlaznu datoteku za pisanje
with open("sortirano.txt", "w") as izlaz:
    # Prolazimo kroz sortiranu listu
    for oib, prezime_ime in osobe:
        # Upisujemo podatke u izlaznu datoteku
        izlaz.write(oib + " " + prezime_ime + "\n")

with open("sortirano.txt", "r") as izlaz:
    for line in izlaz:
        print(line)

-----------------------------------------------------

3.


import os

# Funkcija za sortiranje prema kalorijama
def sort_kalorije(item):
    return int(item[1])

# Inicijaliziramo listu za spremanje podataka
jela = []

# Otvaramo ulaznu datoteku
with open("kalorije.txt", "r") as ulaz:
    # Prolazimo kroz svaku liniju u datoteci
    for linija in ulaz:
        # Razdvajamo naziv jela i broj kalorija
        naziv, kalorije = linija.strip().split(" ")
        # Dodajemo podatke u listu
        jela.append((naziv, kalorije))

# Sortiramo listu prema broju kalorija
jela.sort(key=sort_kalorije, reverse=True)

# Otvaramo izlaznu datoteku za pisanje
with open("sortirano1.txt", "w") as izlaz:
    # Prolazimo kroz sortiranu listu
    for naziv, kalorije in jela:
        # Upisujemo podatke u izlaznu datoteku
        izlaz.write(naziv + " " + kalorije + "\n")

with open("sortirano1.txt", "r") as izlaz:
    for line in izlaz:
        print(line)
        
        
