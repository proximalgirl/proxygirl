def vertikalana_udaljenost(y_1,y_2):
    vd = abs(y_1-y_2)
    return vd

x_1, y_1 = int(input("Molimo uneiste (x_1):")), int(input("Molimo unesite (y_1):"))
x_2, y_2 = int(input("Molimo uneiste (x_2):")), int(input("Molimo unesite (y_2):"))

print("Vertikalna udaljenost unesenih točaka je:", vertikalana_udaljenost(y_1,y_2))

