from random import *

unos = int(input("Molimo unesite cijeli broj (n>0):"))

lista_1 = []
lista_2 = []
lista_3 = []


for i in range(unos*2):
    lista_1.append(randint(1,5))
    lista_2.append(randint(1,5))
    lista_3.append(randint(1,5))

for nubmer in lista_1:
    prosjek_1 = round(sum(lista_1) / len(lista_1),2)
    prosjek_2 = round(sum(lista_2) / len(lista_2),2)
    prosjek_3 = round(sum(lista_3) / len(lista_3),2)

'''
#sustav provjere sipravnosti programa

print(lista_1)
print(lista_2)
print(lista_3)

print(prosjek_1)
print(prosjek_2)
print(prosjek_3)
'''

if prosjek_1 is max(prosjek_1, prosjek_2,prosjek_3):
    print(lista_1)
elif prosjek_2 is max(prosjek_1, prosjek_2,prosjek_3):
    print(lista_2)
elif prosjek_3 is max(prosjek_1, prosjek_2,prosjek_3):
    print(lista_3)
else:
    print("Pogrešan unos")


