1.


import random

while True:
    n = int(input("Upiši broj veći od 0: "))
    if n > 0:
        break

# liste koje sadrze nasumicne ocjene
liste_ocjena = []

for i in range(3):
    trenutna_lista = []
    for j in range(2*n):
        trenutna_lista.append(random.randint(1, 5))
    liste_ocjena.append(trenutna_lista)

#najveci prosjek
najveci_prosjek = 0
najveca_lista = 0
for i in range(3):
    trenutni_prosjek = sum(liste_ocjena[i])/2*n
    if trenutni_prosjek > najveci_prosjek:
        najveci_prosjek = trenutni_prosjek
        najveca_lista = i

print("Lista sa najvećim prosjekom je")
print(liste_ocjena[najveca_lista])
print(",a pripadni prosjek iznosi ", najveci_prosjek)


