def broj_rijeci(s):
    # izdvajanje riječi iz stringa
    rijeci = s.split()
    # vraćanje broja riječi
    return len(rijeci)

# unos stringa od korisnika
s = input("Unesite niz znakova: ")

# poziv funkcije i ispis rezultata
broj = broj_rijeci(s)
print("Broj riječi u unesenom nizu je:", broj)

