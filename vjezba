from turtle import *

unos = int(input("Molimo unesite prirodan broj između 50 i 100:"))

pu()
goto(-unos, -unos)
pd()
for i in range(4):
    fd(unos*2)
    rt(90)

pu()
goto(0,-unos*3)
pd()
fillcolor("black")
begin_fill()
circle(unos)
end_fill()

done()


