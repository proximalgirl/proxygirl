import random

while True:
    n = int(input("Upiši broj veći od 0: "))
    if n > 0:
        break

# liste koje sadrze nasumicne brojeve
liste_brojeva = []

for i in range(n):
    trenutna_lista = []
    for j in range(5):
        trenutna_lista.append(random.randint(0, 10))
    liste_brojeva.append(trenutna_lista)

#najveca suma
najveca_suma = 0
najveca_lista = 0
for i in range(n):
    trenutna_suma = sum(liste_brojeva[i])
    if trenutna_suma > najveca_suma:
        najveca_suma = trenutna_suma
        najveca_lista = i

print("Lista sa najvećom sumom:")
print(liste_brojeva[najveca_lista])
print("Suma elemenata u ovoj listi je: ",najveca_suma)

