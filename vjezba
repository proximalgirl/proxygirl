6. 


from random import *

def izvuci_broj():

    return randint(0,10)


unos = 0
broj_pokusaja = 0

izvuceni_broj = izvuci_broj()
print(izvuceni_broj)

while unos != izvuceni_broj:
    broj_pokusaja += 1
    unos = int(input("{}. pokušaj: Molimo unesite broj između 1 i 10:".format(broj_pokusaja)))
    if unos == izvuceni_broj:
        print("pogodak, pogolili ste iz",broj_pokusaja,". puta.")
        break
    if unos == 0:
        break
    if unos != izvuceni_broj:
        print("Niste pogodlili, molimo pokušajte ponovno")
        
        
        
       
