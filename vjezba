import random


while True:
    n = int(input("Upiši broj veći od 0: "))
    if n > 0:
        break

       
liste = []
for i in range(5):
    trenutna_lista = []
    for j in range(n):
        trenutna_lista.append(random.randint(0, 10))
    liste.append(trenutna_lista)



najveca_suma = 0
najveca_lista = 0
for i in range(5):
    trenutna_suma = sum(liste[i])
    if trenutna_suma > najveca_suma:
        najveca_suma = trenutna_suma
        najveca_lista = i

print("Ispis svih listi:")
for i in range(5):
    print(liste[i])
    
print("Lista sa najvećom sumom elemenata:")
print(liste[najveca_lista])

