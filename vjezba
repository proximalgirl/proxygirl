4.  

import math

def udaljenost_od_ishodista(T):
    x, y = T
    return math.sqrt(x**2 + y**2)

T = (float(input("Unesite x koordinatu: ")), float(input("Unesite y koordinatu: ")))
print("Udaljenost od ishodišta: ", udaljenost_od_ishodista(T))

