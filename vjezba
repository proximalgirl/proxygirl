3. 



f1 = open("ulaz.txt", "r")
f2 = open("izlaz.txt", "w")

lista = []
for line in f1:
    naziv, kalorije = line.split()
    lista.append(f"{naziv} {kalorije}")

lista.sort()
for i, jelo in enumerate(lista):
    print(f"{i+1}. {jelo}", file=f2)

f1.close()
f2.close()


