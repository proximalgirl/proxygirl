2. 

def interpunkcija(s):
    # Funkcija koja broji koliko interpunkcijskih znakova postoji u zadanom stringu
    tocke = s.count(".")
    zarezi = s.count(",")
    dvotočke = s.count(":")
    točka_zarezi = s.count(";")
    return tocke, zarezi, dvotočke, točka_zarezi

# Glavni dio programa
string = input("Unesite niz znakova: ")
tocke, zarezi, dvotočke, točka_zarezi = interpunkcija(string)
print("U unesenom nizu ima:")
print(f"{tocke} točaka")
print(f"{zarezi} zarez")
print(f"{dvotočke} dvotočke")
print(f"{točka_zarezi} točka-zarez")



